<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <context-param>
  	<param-name>contextConfigLocation</param-name>
  	<param-value>classpath:/beans.xml</param-value>
  </context-param>
  <listener>
  	<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
  	<servlet-name>createProductServlets</servlet-name>
  	<display-name>createProductServlets</display-name>
  	<description></description>
  	<servlet-class>com.vishal.product.servlets.createProductServlets</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CreateProductServlet</servlet-name>
  	<display-name>CreateProductServlet</display-name>
  	<description></description>
  	<servlet-class>com.vishal.products.servlets.CreateProductServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DisplayProductsServlet</servlet-name>
  	<display-name>DisplayProductsServlet</display-name>
  	<description></description>
  	<servlet-class>com.vishal.products.servlets.DisplayProductsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>createProductServlets</servlet-name>
  	<url-pattern>/createProductServlets</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CreateProductServlet</servlet-name>
  	<url-pattern>/CreateProductServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DisplayProductsServlet</servlet-name>
  	<url-pattern>/DisplayProductsServlet</url-pattern>
  </servlet-mapping>
</web-app>
